{
	"manifest_version": 3,
	"name": "Statisfy",
	"description": "Statisfy is a cross-browser extension that allows your to show off your browser stats in Discord.",
	"version": "2.0.7",
	"icons": {
		"96": "assets/chat_bubble-white-48dp/2x/outline_chat_bubble_white_48dp.png",
		"36": "assets/chat_bubble-white-18dp/2x/outline_chat_bubble_white_18dp.png"
	},
	"permissions": ["tabs"],
	"host_permissions": ["*://localhost/*"],
	"background": {
		"scripts": ["background.js"]
	},
	"action": {
		"default_icon": {
			"96": "assets/chat_bubble-white-48dp/2x/outline_chat_bubble_white_48dp.png",
			"36": "assets/chat_bubble-white-18dp/2x/outline_chat_bubble_white_18dp.png"
		},
		"default_title": "Statisfy"
	}
}
