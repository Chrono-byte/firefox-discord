{
	"description": "Adds Discord Rich Presence to Firefox",
	"manifest_version": 2,
	"name": "discord-rich-presence",
	"version": "1.0.0",
	"permissions": [
		"<all_urls>", "tabs", "http://localhost:1337/*"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"applications": {
		"gecko": {
			"id": "discord-rich-presence@luxvacuos.net",
			"strict_min_version": "42.0"
		}
	}  
}